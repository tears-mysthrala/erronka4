parameters:
    level: 6
    paths:
        - src
    excludePaths:
        - src/Config/routes.php
    checkMissingIterableValueType: false
    checkGenericClassInNonGenericObjectType: false
    ignoreErrors:
        # Ignore session-related warnings (handled by framework)
        - '#Undefined variable: \$_(SESSION|SERVER|POST|GET)#'
